{
    "model": "squeezenet1_1",
    "pretrained": false,
    "input_info": {
      "sample_size":  [1, 3, 32, 32]
    },
    "batch_size": 512,
    "dataset": "CIFAR10",
    "epochs": 5,
    "num_classes": 10,
    "optimizer": {
        "type": "sgd",
        "base_lr": 1e-2,
        "schedule_type": "multistep",
        "steps": [
            1
        ]
    },
    "compression": [
        {
            "algorithm": "rb_sparsity",
            "sparsity_init": 0.2,
            "params": {
                "schedule": "polynomial",
                "sparsity_target": 0.6,
                "sparsity_target_epoch": 5
            }
        },
        {
            "algorithm": "quantization",
            "weights": {
                "per_channel": true
            },
            "initializer": {
                "range": {
                    "num_init_samples": 1
                }
            }
        }
    ]
}
